{
  "name": "yambo",
  "version": "1.0.0",
  "description": "softwate para gestion de contabilidad y pedidos (build with pkg)",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "start": "nodemon .",
    "test": "./__main__.py",
    "dbc": "node module/dbConfig.js",
    "jsx": "npx babel --watch src --out-dir public/js/jsxbuild --presets react-app/prod",
    "user": "node module/addUser.js"
  },
  "keywords": [
    "yambo",
    "laguna",
    "pedidos"
  ],
  "author": "Julio Castro (NOVA)",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "cookie-session": "^1.3.3",
    "ejs": "^2.6.1",
    "ejs-locals": "^1.0.2",
    "escpos": "^2.4.11",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "https": "^1.0.0",
    "mongodb": "^3.2.4",
    "multer": "^1.4.1",
    "path": "^0.12.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.2"
  },
  "pkg": {
    "scripts": [
      "models/*",
      "module/*",
      "rout/*"
    ],
    "assets": [
      "assets/*",
      "assets/family/*",
      "assets/producto/*",
      "assets/subfamily/*",
      "public/css/*",
      "public/js/*",
      "public/js/jsxbuild/*",
      "temp/*",
      "views/*",
      "views/rootDir/*",
      "views/rootDir/adminDir/*",
      "views/rootDir/configDir/*",
      "views/rootDir/pedDir/*",
      "node_modules/react/umd",
      "node_modules/bootstrap/dist/css",
      "node_modules/react-dom/umd"
    ],
    "targets": [
      "node10-linux-x64", 
      "node10-win-x64"
    ]
  }
}
